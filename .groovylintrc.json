{
    "extends": "recommended",
    "rules": {
        "CompileStatic": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "JUnitPublicNonTestMethod": {
            "enabled": false
        },
        "MethodName": {
            "enabled": false
        },
        "MethodParameterTypeRequired": {
            "enabled": false
        },
        "MethodReturnTypeRequired": {
            "enabled": false
        },
        "NoDef": {
            "enabled": false
        },
        "PrivateFieldCouldBeFinal": {
            "enabled": false
        },
        "UnnecessaryDefInMethodDeclaration": {
            "enabled": false
        },
        "UnnecessaryGetter": {
            "enabled": false
        },
        "VariableTypeRequired": {
            "enabled": false
        }
    }
}